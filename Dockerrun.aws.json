{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions":[
        {
            "name": "client",
            "image": "javaxiss/multi-client",
            "hostname": "client",
            "essential": false
        },
        {
            "name": "server",
            "image": "javaxiss/multi-server",
            "hostname": "api",
            "essential": false
        },
        {
            "name":"worker",
            "image": "javaxiss/multi-worker",
            "hostname": "worker",
            "essential": false
        },
        {
          "name": "nginx",
          "image": "javaxiss/multi-nginx",
          "hostname": "nginx", 
          "essential": true,
          "portMappings": [
              {
                  "hostPort": 80,
                  "containerPort": 80
              }
          ],
          "links":["server", "client"]
        }
    ]
}